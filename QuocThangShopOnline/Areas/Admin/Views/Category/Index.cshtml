@model List<QuocThangShopOnline.Admin.Models.CategoryViewModel>

@using (Html.BeginForm("Index", "Category", FormMethod.Post))
    {
   
    <table cellpadding="0" cellspacing="0">
            <tr>
                <td>Id</td>
                <td>Name</td>
            </tr>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.Id</td>
                <td>@category.Name</td>
            </tr>
        }
    </table>
    }